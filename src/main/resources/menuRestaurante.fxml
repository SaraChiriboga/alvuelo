<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>


<StackPane fx:id="stackContentArea" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MenuRestauranteController">
   <children>
      <AnchorPane prefHeight="812.0" prefWidth="375.0" style="-fx-background-image: url('/bckgrnd.png');">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <HBox fx:id="bar" alignment="CENTER" layoutX="30.0" layoutY="720.0" prefHeight="61.0" prefWidth="316.0" spacing="40.0" style="-fx-background-color: c23939;" stylesheets="@main-style.css">
               <children>
                  <ImageView fx:id="homeButton" fitHeight="30.0" fitWidth="28.0" onMouseClicked="#home" pickOnBounds="true" preserveRatio="true" smooth="false" style="-fx-opacity: 50;">
                     <image>
                        <Image url="@store.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="41.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true" smooth="false">
                     <image>
                        <Image url="@shoppingcar.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="41.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true" smooth="false">
                     <image>
                        <Image url="@user.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
            <AnchorPane prefHeight="142.0" prefWidth="375.0" style="-fx-background-color: c23939;" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
